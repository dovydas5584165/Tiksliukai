datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id          Int      @id @default(autoincrement())
  email       String   @unique
  password    String
  name        String
  vaikoVardas String
  pamokos     String[]  
  role        String   
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}


